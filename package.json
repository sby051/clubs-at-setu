{
	"name": "clubs-at-setu",
	"version": "0.0.2",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"dev:host": "vite dev --host",
		"env:pull": "npx dotenv-vault pull",
		"env:push": "npx dotenv-vault push",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"build": "vite build",
		"preview": "vite preview",
		"lint": "eslint .",
		"deploy": "vercel deploy --prod --yes",
		"fmt": "prettier --write --plugin-search-dir=. ./src/**/*.{js,ts,html,css,svelte,json}"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^1.0.2",
		"@sveltejs/kit": "^1.3.7",
		"@tailwindcss/typography": "^0.5.9",
		"@typescript-eslint/eslint-plugin": "^5.50.0",
		"@typescript-eslint/parser": "^5.45.0",
		"autoprefixer": "^10.4.7",
		"eslint": "^8.28.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.14",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.3",
		"prettier-plugin-organize-imports": "^3.2.2",
		"prettier-plugin-svelte": "^2.9.0",
		"prettier-plugin-tailwindcss": "^0.2.2",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.1.5",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.0"
	},
	"type": "module",
	"dependencies": {
		"firebase": "^9.16.0",
		"material-symbols": "^0.4.3",
		"svelte-emoji-selector": "^1.0.1",
		"svelte-time": "^0.7.1",
		"sveltils": "^0.2.0",
		"vercel": "^28.14.1"
	}
}
